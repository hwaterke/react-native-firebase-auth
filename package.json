{
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "format": "prettier --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "expo": "^25.0.0",
    "react": "16.2.0",
    "react-native":
      "https://github.com/expo/react-native/archive/sdk-25.0.0.tar.gz"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1"
  },
  "lint-staged": {
    "*.js": ["prettier --write", "eslint --max-warnings 0", "git add"],
    "*.{css,scss,json,md}": ["prettier --write", "git add"]
  }
}
